{
  "name": "werkbank",
  "version": "0.1.0",
  "description": "A simple tool to manage and store work pieces",
  "license": "ISC",
  "author": "Simon Sedlatschek <code@sedlatschek.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sedlatschek/werkbank.git"
  },
  "scripts": {
    "serve": "yarn electron:serve",
    "build": "yarn raw:build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "raw:serve": "vue-cli-service serve",
    "raw:build": "vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps && yarn generate-disclaimer",
    "postuninstall": "electron-builder install-app-deps && yarn generate-disclaimer",
    "release": "electron-builder --windows --linux --publish always",
    "generate-disclaimer": "yarn licenses generate-disclaimer --silent > ./src/assets/disclaimer.txt"
  },
  "main": "background.js",
  "dependencies": {
    "@akryum/winattr": "^3.0.0",
    "@mdi/font": "5.9.55",
    "adm-zip": "^0.5.5",
    "electron": "^13.1.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-updater": "^4.3.9",
    "fs-extra": "^10.0.0",
    "raw-loader": "^4.0.2",
    "roboto-fontface": "*",
    "slugify": "^1.5.3",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-electron-store": "^1.4.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "electron-builder": "^22.11.7",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "spectron": "15.0.0",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  }
}
